<?xml version="1.0" encoding="utf-8"?>
<layout type="lm">	
	<head>
	</head>
	<groups>
		<group class="span3" data-normal="" data-tablet="" data-stablet="">left</group>
		<group
			class="span9" 
			data-normal="span9" 
			data-stablet="span9"
			
			noleft-class="span12" 
			noleft-data-normal="span12"  
			noleft-data-stablet="span12">main</group>
	</groups>
	<blocks>			
		<header html5tag="header" order="1" id="yt_header" autosize="0">
			<positions>						
				<position class="span9 offset3" data-normal="span9 offset3" data-tablet="span9 offset3" data-stablet="span6" type="feature">@menu</position>		
				<position type="feature">@logo</position>				
				<position class="span6" data-normal="span4 hidden" data-tablet="span4 hidden" data-stablet="span6" style="none" type="modules">top-3</position>							
			</positions>
		</header>
		
		<spotlight html5tag="section" order="2" id="yt_spotlight">
			<positions>
				<position class="span4 offset4" data-normal="span5 offset2" data-tablet="span5 offset2" data-stablet="span12" style="none" type="modules">top-1</position>
				<position class="span4" data-normal="span5" data-tablet="span5" data-stablet="span5 hidden" style="none" type="modules">top-2</position>							
			</positions>
		</spotlight>
		
		<spotlight1 html5tag="section" order="3" id="yt_spotlight1">
			<positions>
				<position class="span12" style="ytmod" type="modules">position-0</position>					
			</positions>
		</spotlight1>
		
		<spotlight2 html5tag="section" order="4" id="yt_spotlight2">
			<positions>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">position-a</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">position-b</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">position-c</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">position-d</position>			
			</positions>
		</spotlight2>
		
		<content html5tag="section" order="6" id="content" showDivTop="0" showDivBottom="0">
			<positions>
				<position class="span12" data-normal="" data-tablet="" data-stablet="" group="left" style="ytmod" type="modules">position-6</position>
				<position class="span12" group="main" style="ytmod" type="modules">breadcrumb</position>
				<position 
					class="span12 no-minheight" 
					data-normal="" 
					data-stablet=""
					
					noleft-class="span12" 
					noleft-data-normal="" 
					noleft-data-tablet="" 
					noleft-data-stablet="" 
					
					group="main" type="message"></position>
				<position 
					class="span12" 
					data-normal="" 
					data-stablet=""
					
					noleft-class="span12" 
					noleft-data-normal="" 
					noleft-data-tablet="" 
					noleft-data-stablet="" 					
					 
					group="main" type="component">component</position>
				<position
					class="span12"
					data-normal="span12"
					data-stablet="span12" 
					  
					noleft-class="span12" 
					noleft-data-normal="" 
					noleft-data-stablet=""
					  
					group="main" style="ytmod2" type="modules">position-5</position>	
			</positions>
		</content>
		
		<spotlight3 html5tag="section" order="8" id="yt_spotlight3">
			<positions>
				<position class="span6" data-normal="" data-stablet="" style="ytmod" type="modules">position-1</position>	
				<position class="span6" data-normal="" data-stablet="" style="ytmod" type="modules">position-2</position>				
			</positions>
		</spotlight3>
		<spotlight4 html5tag="section" order="9" id="yt_spotlight4">
			<positions>
				<position class="span6" data-normal="" data-stablet="" style="ytmod" type="modules">position-3</position>	
				<position class="span6" data-normal="" data-stablet="" style="ytmod" type="modules">position-4</position>
			</positions>
		</spotlight4>
		
		<spotlight9 html5tag="section" order="11" id="yt_spotlight9">
			<positions>
				<position class="span12" data-normal="" data-stablet="" style="ytmod2" type="modules">position-16</position>		
			</positions>
		</spotlight9>
		
		<spotlight5 html5tag="section" order="12" id="yt_spotlight5">
			<positions>
				<position class="span12" data-normal="" data-stablet="" style="ytmod2" type="modules">position-15</position>		
			</positions>
		</spotlight5>
		
		<spotlight8 html5tag="section" order="13" id="yt_spotlight8">
			<positions>
				<position class="span12" data-normal="" data-stablet="" style="ytmod" type="modules">position-8</position>		
			</positions>
		</spotlight8>
		
		<spotlight6 html5tag="section" order="14" id="yt_spotlight6">
			<positions>					
				<position class="span8" data-normal="span8"  data-tablet="span8"  data-stablet="span8" style="ytmod" type="modules">position-9</position>
				<position class="span4" data-normal="span4" data-tablet="span4" data-stablet="span4" style="ytmod" type="modules">position-10</position>				
			</positions>
		</spotlight6>
		<spotlight7 html5tag="section" order="15" id="yt_spotlight7">
			<positions>						
				<position class="span3" data-normal="" data-stablet="span6" style="ytmod" type="modules">position-11</position>
				<position class="span3" data-normal="" data-stablet="span6" style="ytmod" type="modules">position-12</position>
				<position class="span3" data-normal="" data-stablet="span6" style="ytmod" type="modules">position-13</position>
				<position class="span3" data-normal="" data-stablet="span6" style="ytmod" type="modules">position-14</position>					
			</positions>
		</spotlight7>
		
		<footer html5tag="footer" order="16" id="yt_footer">
			<positions>			
			<position class="span12" type="feature">@copyright</position>
			</positions>			
		</footer>		
									
	</blocks>
</layout>