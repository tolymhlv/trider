<?xml version="1.0" encoding="utf-8"?>
<layout type="mlr">	
	<head>
	</head>
	<groups>
		<group class="span3" data-normal="" data-tablet="span3" data-stablet="">left</group>
		<group class="span3" data-normal="" data-tablet="span3" data-stablet="" >right</group>
		<group 
			   class="span6 " 
			   data-normal="span6" 
			   data-tablet="span6"			  
			   
			   noright-class="span9"
			   noright-data-normal="" 
			   noright-data-stablet="span9"
			   
			   noleft-class="span9 "
			   noleft-data-normal="" 
			   noleft-data-stablet=""
			   
			   noleftright-class="span12"
			   noleftright-data-normal="" 
			   noleftright-data-tablet="" 
			   noleftright-data-stablet="">main</group>
	</groups>
	<blocks>			
		<header html5tag="header" order="1" id="yt_header" autosize="0">
			<positions>
				<position class="span3 offset5" data-normal="span3 offset5" data-tablet="span4 offset4" data-stablet="span4 offset3" style="none" type="modules">top-1</position>
				<position class="span4" data-normal="span4" data-tablet="span4" data-stablet="span5" style="none" type="modules">top-2</position>				
			</positions>
		</header>
		
		<menu html5tag="section" order="2" id="yt_menuwrap">
			<positions>
				<position type="feature" class="">@logo</position>
				<position class="span10 offset2" data-normal="span9 offset3" data-tablet="span9 offset3" data-stablet="span6 first" type="feature">@menu</position>	
				<position class="span6" data-normal="span6 hidden" data-tablet="span6 hidden" data-stablet="span6" style="ytmod" type="modules">top-3</position>			
			</positions>
		</menu>
		
		<spotlight1 html5tag="section" order="3" id="yt_spotlight1">
			<positions>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">position-a</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">position-b</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">position-c</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">position-d</position>
			</positions>
		</spotlight1>	
		
		<spotlight2 html5tag="section" order="4" id="yt_spotlight2">
			<positions>
				<position class="span12" style="ytmod" type="modules">position-0</position>				
			</positions>
		</spotlight2>	
		
		<content html5tag="section" order="6" id="content" showDivTop="0" showDivBottom="0">
			<positions>
				<position class="span12" group="main" style="ytmod" type="modules">breadcrumb</position>
				<position 
					class="span12 no-minheight" 
					data-normal="" 
					data-stablet=""
					
					noleft-class="span12" 
					noleft-data-normal="" 
					noleft-data-tablet="" 
					noleft-data-stablet="" 
					
					noright-class="span12" 
					noright-data-normal="" 
					noright-data-tablet="" 
					noright-data-stablet="" 
					
					noleftright-class="span12" 
					noleftright-data-normal="" 
					noleftright-data-tablet="" 
					noleftright-data-stablet="" 
					
					group="main" type="message"></position>
				<position 
					class="span12" 
					data-normal="" 
					data-stablet=""
					
					noleft-class="span12" 
					noleft-data-normal="" 
					noleft-data-tablet="" 
					noleft-data-stablet="" 
					
					noright-class="span12" 
					noright-data-normal="" 
					noright-data-tablet="" 
					noright-data-stablet="" 
					
					noleftright-class="span12" 
					noleftright-data-normal="" 
					noleftright-data-tablet="" 
					noleftright-data-stablet="" 
					 
					group="main" type="component">component</position>
				<position
					class="span12"
					data-normal="span12"
					data-stablet="span12" 
					  
					noright-class="span12" 
					noright-data-normal="" 
					noright-data-stablet=""
					  
					group="main" style="ytmod" type="modules">position-5</position>
					
				<position class="span12" data-normal="" data-tablet="" data-stablet="" group="left" style="ytmod" type="modules">position-6</position>
				<position class="span12" data-normal="" data-tablet="" data-stablet="" group="right" style="ytmod" type="modules">position-7</position>
			</positions>
		</content>
		
		<spotlight3 html5tag="section" order="8" id="yt_spotlight3">
			<positions>
				<position class="span3" data-normal="" data-stablet="span3" style="ytmod" type="modules">position-1</position>
				<position class="span3" data-normal="" data-stablet="span3" style="ytmod" type="modules">position-2</position>
				<position class="span3" data-normal="" data-stablet="span3" style="ytmod" type="modules">position-3</position>
				<position class="span3" data-normal="" data-stablet="span3" style="ytmod" type="modules">position-4</position>
			</positions>
		</spotlight3>
		<spotlight4 html5tag="section" order="9" id="yt_spotlight4">
			<positions>
				<position class="span5" data-normal=""  data-tablet="span7"  data-stablet="span7" style="ytmod" type="modules">position-9</position>
				<position class="span3 offset4" data-normal="span4 offset3" data-tablet="span5" data-stablet="span5" style="ytmod" type="modules">position-10</position>				
			</positions>
		</spotlight4>
		<spotlight5 html5tag="section" order="10" id="yt_spotlight5">
			<positions>
				<position class="span2d4" data-normal="" data-stablet="" style="ytmod" type="modules">position-11</position>
				<position class="span2d4" data-normal="" data-stablet="" style="ytmod" type="modules">position-12</position>
				<position class="span2d4" data-normal="" data-stablet="" style="ytmod" type="modules">position-13</position>
				<position class="span2d4" data-normal="" data-stablet="" style="ytmod" type="modules">position-14</position>		
				<position class="span2d4" data-normal="" data-stablet="" style="ytmod" type="modules">position-15</position>		
			</positions>
		</spotlight5>
		<spotlight6 html5tag="section" order="11" id="yt_spotlight6">
			<positions>
				<position class="span12" data-normal="" data-stablet="" style="ytmod" type="modules">position-16</position>					
			</positions>
		</spotlight6>
		<spotlight7 html5tag="section" order="12" id="yt_spotlight7">
			<positions>
				<position class="span12" data-normal="" data-stablet="" style="ytmod" type="modules">position-17</position>					
			</positions>
		</spotlight7>
		
		<footer html5tag="footer" order="13" id="yt_footer">
			<positions>
			<position class="span12" type="feature">@copyright</position>
			</positions>			
		</footer>	
									
	</blocks>
</layout>